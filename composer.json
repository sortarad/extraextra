{
    "name": "sortarad/extraextra",
    "description": "The hottest news right in your Statamic dashboard.",
    "license": "MIT",
    "keywords": [
        "statamic",
        "rss",
        "widget"
    ],
    "type": "library",
    "require": {
        "simplepie/simplepie": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Sortarad\\ExtraExtra\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "ExtraExtra",
            "description": "The hottest news right in your Statamic dashboard."
        },
        "laravel": {
            "providers": [
                "Sortarad\\ExtraExtra\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
